<!DOCTYPE html>
<html lang="<%- config.language %>">
  <%- partial('_partial/head') %>
  <body>
    <%- partial('_partial/header', null, {cache: false, path: path}) %>

    <div id="l_body">
      <%- partial('_partial/cover') %>
      <div id="safearea">
        <div class="body-wrapper" id="pjax-container">
          <%- body %>
		  <% if (page.snackbar && site.data.notification && (page.snackbar in site.data.notification)) { %> 
		    <%- partial('_partial/snackbar') %>
		  <% } %>
		  <%- partial('_third-party/pjax/pdata') %>
        </div>
        
        <%- partial('_partial/footer', null, {cache: !config.relative_link}) %>
       <!-- <a id="s-top" class="fas fa-arrow-up fa-fw" href="javascript:void(0)"></a> -->
        
      </div>
    </div>
    <div>
      <%- partial('_partial/scripts/index') %> 
	  <% if (theme.plugins.pjax.enable){ %> 
	    <%- partial('_third-party/pjax/index') %> 
	  <% } %>
    </div>
    <!--动态线条背景-->
<script type="text/javascript"
color="255,69,0" opacity='0.7' zIndex="-2" count="200" src="//cdn.bootcss.com/canvas-nest.js/1.0.0/canvas-nest.min.js">
</script>
<script src="https://cdn.jsdelivr.net/gh/zyoushuo/Blog/hexo/js/mouse_click.js"></script>

<%- partial('_partial/left', null, {cache: !config.relative_link}) %>
  </body>
</html>
